<job id="batchlet-progress-test" xmlns="https://jakarta.ee/xml/ns/jakartaee" version="2.0">
  <properties>
    <property name="key1" value="#{jobParameters['key1']}" />
    <property name="key2" value="これはリテラル" />
  </properties>
  <listeners>
    <listener ref="nablarchJobListenerExecutor">
      <properties>
        <property name="diConfigFilePath" value="#{jobParameters['diConfig']}" />
      </properties>
    </listener>
  </listeners>

  <step id="step">
    <listeners>
      <listener ref="nablarchStepListenerExecutor" />
    </listeners>
    <batchlet ref="progressBatchlet" />
  </step>
</job>